{
  "name"        : "aws-serverless-boilerplate",
  "version"     : "0.7.0",
  "description" : "Template for common services to provide the re-usability like logs, serverless yml file template, basic utility functions, nodejs  and services for the AWS.",
  "main"        : "index.js",
  "dependencies": {
    "link-module-alias": "^1.2.0"
  },
  "devDependencies": {
    "aws-sdk"                       : "^2.614.0",
    "serverless-offline"            : "^5.12.1",
    "serverless-offline-scheduler"  : "^0.4.0",
    "serverless-plugin-split-stacks": "^1.9.3",
    "serverless-plugin-typescript"  : "^1.1.9",
    "serverless-s3-deploy"          : "^0.9.0",
    "serverless-secrets-plugin"     : "^0.1.0",
    "typescript"                    : "^3.7.5"
  },
  "scripts": {
    "debug"      : "export SLS_DEBUG=* && node --inspect=5858 /usr/local/bin/serverless offline -s dev -v",
    "start"      : "/usr/local/bin/serverless offline start -s dev ",
    "prestart"  : "link-module-alias",
    "test"       : "echo \"Error: no test specified\" && exit 1",
    "preinstall" : "which link-module-alias && link-module-alias clean || echo",
    "postinstall": "link-module-alias"
  },
  "_moduleAliases": {
    "@services": ".build/src/services",
    "@utils"   : ".build/src/utils",
    "~src"     : ".build/src"
  },
  "keywords": [
    "aws",
    "serverless",
    "nodejs",
    "asif"
  ],
  "author" : "Asif",
  "license": "ISC"
}